{
  "expo": {
    "name": "Tab Navigation Template",
    "slug": "WeddingApp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "notification": {
      "icon": "./assets/images/crown.png",
      "color": "blue",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    },
    "orientation": "portrait",
    "icon": "./assets/images/crown.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/crown.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
